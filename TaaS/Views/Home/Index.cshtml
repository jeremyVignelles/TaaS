@model Dictionary<Guid, TimelapseInfo>
<!DOCTYPE html>
<html>
<head>
	<title>Timelapse as a Service</title>
	<style>
		.button 
		{
			background: blue;
			height: 48px;
			width: 48px;
			display: flex;
			color: white;
			position: fixed;
			bottom: 10px;
			right: 10px;
			border-radius: 50%;
			box-shadow: 1px 1px black;
			justify-content: center;
			align-items: center;
			text-decoration: none;
			font-weight: bold;
			font-size: 1.5em;

		}
	</style>
</head>
<body>
@foreach (var timelapseKeyValuePair in Model)
{
	<a href="/Timelapse/@timelapseKeyValuePair.Key">@timelapseKeyValuePair.Value.Name</a>
}
<!-- bouton + -->
<a class="button" href="/Home/Create">+</a>
</body>
</html>