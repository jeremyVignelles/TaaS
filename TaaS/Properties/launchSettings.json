{
  "profiles": {
    "TaaS": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:7079;http://localhost:5079",
      "dotnetRunMessages": true
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": false,
      "publishAllPorts": true,
      "useSSL": true,
      "DockerfileRunArguments": "-v taas_data:/data",
      "environmentVariables": {
        "DataFolder": "/data"
      }
    }
  }
}